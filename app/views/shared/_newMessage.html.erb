<%# Fuctionality to create new message %>
<%= form_for @new_message do |f|%>

  <div class="messageFlash"><%= flash[:messageFlash] %></div>

  <div class="messageForm">
    <%= f.label :content, "800 character maximum" %>
    <br>
    <%= f.text_area :content %>
  </div>

  <div>
    <br>
    <%= f.submit "Submit" %>
  </div>

<% end %>